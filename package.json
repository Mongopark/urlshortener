    "react-dom": "^18.2.0",
    "react-icons": "^5.1.0",
    "react-redux": "^9.1.1",
    "react-responsive": "^10.0.0",
    "react-router-dom": "^6.23.0",
    "react-toastify": "^10.0.5",
    "redux-persist": "^6.0.0",
    "yup": "^1.4.0"
